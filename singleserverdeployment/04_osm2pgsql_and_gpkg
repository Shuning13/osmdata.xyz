#This script imports each osm.pbf part to postgis database and creates extracts. Afterwards, the tables are dropped.
#please check how much RAM and CPU cores your machine has. Please adjust "-C" regarding RAM and "--number-processes" regarding CPU cores
#Don't forget to set the passwords for the database user
#Run this script as osmdata user

osm2pgsql --slim -r pbf -S mapfeatures.style -C 14000 -l --number-processes 4 -U osmdata --create --multi-geometry --keep-coastlines -d osmdatabase planet-latest_aerialway.osm.pbf
ogr2ogr -f "GPKG" /home/osmdata/gpkg/aerialway_EPSG4326_point.gpkg PG:"host=localhost dbname=osmdatabase user=osmdata password=*******" -sql "select * from planet_osm_point where aerialway is not null"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/aerialway_EPSG4326_line.gpkg PG:"host=localhost dbname=osmdatabase user=osmdata password=*******" -sql "select * from planet_osm_line where aerialway is not null"
ogr2ogr -f "GPKG" /home/osmdata/gpkg/aerialway_EPSG4326_polygon.gpkg PG:"host=localhost dbname=osmdatabase user=osmdata password=*******" -sql "select * from planet_osm_polygon where aerialway is not null"
psql -d osmdatabase -c "DROP TABLE planet_osm_point"
psql -d osmdatabase -c "DROP TABLE planet_osm_line"
psql -d osmdatabase -c "DROP TABLE planet_osm_polygon"
psql -d osmdatabase -c "DROP TABLE planet_osm_nodes"
psql -d osmdatabase -c "DROP TABLE planet_osm_rels"
psql -d osmdatabase -c "DROP TABLE planet_osm_roads"
psql -d osmdatabase -c "DROP TABLE planet_osm_ways"

osm2pgsql --slim -r pbf -S mapfeatures.style -C 14000 -l --number-processes 4 -U osmdata --create --multi-geometry --keep-coastlines -d osmdatabase planet-latest_aeroway.osm.pbf
